{
	"name": "onedrive-browser",
	"displayName": "OneDrive Browser",
	"version": "0.0.1",
	"private": false,
	"description": "Browse and edit files from OneDrive in VS Code",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandOnedriveBrowser#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandOnedriveBrowser/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandOnedriveBrowser.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"browser": "./dist/web/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile-web": "webpack",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
		"watch-web": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "onedrive-browser.openOneDrive",
				"title": "Open OneDrive Folder"
			}
		],
		"webOpener": {
			"import": "webOpener.js",
			"scheme": "onedrive"
		}
	},
	"activationEvents": [
		"onCommand:onedrive-browser.openOneDrive",
		"onFileSystem:onedrive"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"assert": "2.1.0",
		"process": "0.11.10",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"preview": true
}
